{
  "name": "react-ts-boilerplate",
  "private": true,
  "version": "1.3.0",
  "author": "Emanuela Coutinho",
  "license": "MIT License",
  "description": "Boilerplate vite, react, ts",
  "bugs": {
    "url": "https://github.com/ManuCoutinho/vite-react-ts-template"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ManuCoutinho/vite-react-ts-template"
  },
  "keywords": [
    "vitejs",
    "react",
    "typescript",
    "jest",
    "boilerplate"
  ],
  "engines": {
    "node": ">=18.16",
    "npm": ">=9.5.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx src",
    "lint:css": "stylelint \"src/**/*.css\"",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint src --fix",
      "jest --findRelatedTests --passWithNoTests --bail"
    ],
    "*.{json,md, yaml}": [
      "prettier --write"
    ],
    "*.css": [
      "stylelint --fix './src/**/*.css'"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@swc/core": "^1.3.27",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.0",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "lint-staged": "^13.1.0",
    "prettier": "^3.0.0",
    "semantic-release": "^20.0.2",
    "stylelint": "^15.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^33.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.0",
    "vite": "^4.3.9"
  }
}
